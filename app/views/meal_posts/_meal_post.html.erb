<div id="<%= dom_id meal_post %>">
  <p>
    <strong>Title:</strong>
    <%= meal_post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= meal_post.content %>
  </p>

  <p>
    <strong>Meal type:</strong>
    <%= meal_post.meal_type %>
